		"Backpacks can be dyed and upgraded to higher tiers for more capacity."
	]

	quest.0E057B7F76401421.title: "Copper Backpack"
	quest.0E057B7F76401421.quest_desc: ["Optional intermediate tier. Can skip directly to iron backpack."]

	quest.2F9B0C642A6BE30C.title: "Iron Backpack"
	quest.2F9B0C642A6BE30C.quest_desc: ["Increased storage capacity and upgrade slots."]

	quest.45268A619787288F.title: "Gold Backpack"
	quest.45268A619787288F.quest_desc: ["More capacity and upgrade slots than iron."]

	quest.67704F7341EDCC49.title: "Diamond Backpack"
	quest.67704F7341EDCC49.quest_desc: ["High-capacity backpack with many upgrade slots."]

	quest.46D756F4DC6CC894.title: "Netherite Backpack"
	quest.46D756F4DC6CC894.quest_desc: ["Maximum capacity. Fireproof and survives lava."]

