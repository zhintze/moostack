{
	// Sophisticated Storage & Backpacks Chapter
	// Chapter ID: 1DB294A8F8686321

	// ==========================================
	// HUB QUESTS
	// ==========================================
	quest.5A94A2664BFDD7B9.title: "Storage Basics"
	quest.5A94A2664BFDD7B9.quest_desc: ["This chapter covers item storage options from vanilla chests to upgradeable Sophisticated Storage containers and backpacks."]

	quest.7EF57BBEAA4B6B08.title: "NBT Items"
	quest.7EF57BBEAA4B6B08.quest_desc: [
		"Enchanted items, tools with durability, and other NBT-tagged items don't stack. Storing many of these in virtual storage systems (like AE2) can cause performance issues."
		""
		"Store NBT items in physical containers like chests or backpacks instead."
	]
	task.21FBC4E0F668347C.title: "I understand NBT storage"

	// ==========================================
	// VANILLA STORAGE
	// ==========================================
	quest.134608BB2B04D96E.title: "Chest"
	quest.134608BB2B04D96E.quest_desc: ["Vanilla chests hold 27 slots. Place two adjacent chests to form a double chest with 54 slots."]

	quest.7F990FBC1A691020.title: "Trapped Chest"
	quest.7F990FBC1A691020.quest_desc: ["Emits a redstone signal when opened. Cannot be combined into double chests."]

	quest.1659A59303C15CC9.title: "Barrel"
	quest.1659A59303C15CC9.quest_desc: ["Functions like a chest with 27 slots. Cannot be combined but can be stacked vertically."]

	quest.25AFD10D008DF30B.title: "Ender Chest"
	quest.25AFD10D008DF30B.quest_desc: ["Personal dimensional storage. All your ender chests share the same 27-slot inventory."]

	quest.33AADC1B97E9F7A9.title: "Chest Minecart"
	quest.33AADC1B97E9F7A9.quest_desc: ["Mobile storage on rails. Useful for automated item transport systems."]

	quest.544791880F315D5A.title: "Chest Boat"
	quest.544791880F315D5A.quest_desc: ["Boat with built-in chest storage. Single rider only when chest is attached."]

	// ==========================================
	// SOPHISTICATED STORAGE - TIER PROGRESSION
	// ==========================================
	quest.5E4BC0F59C90433A.title: "Sophisticated Storage"
	quest.5E4BC0F59C90433A.quest_desc: [
		"Upgradeable storage containers that accept functional upgrades. Craft a basic chest or barrel to start."
		""
		"Higher tier containers hold more items and support more upgrade slots."
	]

	quest.0CD04A00529FE1C9.title: "Copper Tier"
	quest.0CD04A00529FE1C9.quest_desc: ["Optional intermediate tier. Saves iron but can be skipped by crafting iron tier directly."]

	quest.58514FDE153FD971.title: "Iron Tier"
	quest.58514FDE153FD971.quest_desc: ["Increased storage capacity. First major upgrade from basic tier."]

	quest.3E33730DC2115D26.title: "Gold Tier"
	quest.3E33730DC2115D26.quest_desc: ["Larger capacity and more upgrade slots than iron tier."]

	quest.4C0BDD483CCB40C4.title: "Diamond Tier"
	quest.4C0BDD483CCB40C4.quest_desc: ["High-capacity storage with multiple upgrade slots."]

	quest.77F241BEE9902751.title: "Netherite Tier"
	quest.77F241BEE9902751.quest_desc: ["Maximum storage capacity. Fireproof and holds the most items."]

	// ==========================================
	// SOPHISTICATED STORAGE - UTILITIES
	// ==========================================
	quest.4007DFA7CC3A5FF2.title: "Storage Upgrade Base"
	quest.4007DFA7CC3A5FF2.quest_desc: ["Required crafting component for all storage upgrades. See the backpack upgrades section for functionality - storage upgrades work the same way."]

	quest.1FE17B1C7C639F88.title: "Storage Controller"
	quest.1FE17B1C7C639F88.quest_desc: [
		"Links multiple Sophisticated Storage containers into a unified network. Acts as a single access point for all connected storage."
		""
		"Connect containers by placing them adjacent to the controller or to other connected containers. Maximum range: 14 blocks from controller."
	]

	quest.78E51D22C16C4686.title: "Storage Tool"
	quest.78E51D22C16C4686.quest_desc: ["Wirelessly links storage containers to the controller. Right-click the controller to bind, then right-click containers to connect them."]

	quest.0108F3EFDFD6DFD0.title: "Storage Link"
	quest.0108F3EFDFD6DFD0.quest_desc: ["Wirelessly connects distant storage containers to your controller network without requiring adjacent placement."]

