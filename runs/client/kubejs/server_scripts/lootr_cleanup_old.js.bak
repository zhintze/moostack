LootJS.lootTables(event => {
    for (let id of CHEST_TABLES) {
        let table = event.getLootTable(id)
        table.clear()
    }
})


LootJS.modifiers(event => {
    // Remove everything GLMs are trying to add
    for (let id of CHEST_TABLES) {
        let table = event.addTableModifier(id)

        table.removeLoot(entry => {
            return entry.isItem() && modsToRemove.some(mod => entry.item.id.startsWith(`${mod}:`))
        })
    }

})

